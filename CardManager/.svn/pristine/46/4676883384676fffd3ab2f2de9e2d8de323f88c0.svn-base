/*V*/
.adminMenu{background:url(../images/admin_14.png) repeat-y left}

ul.adminMenu {display:block; font-size:1px; line-height:1px; margin:0; padding:0}
ul.adminMenu li{font-size:14px; line-height:30px; margin-bottom:2px}
ul.adminMenu li ul{display:none}
ul.adminMenu li a{color:#333; background:url(../images/admin_12.png) no-repeat left; display:block; height:30px; line-height:30px; margin-bottom:1px; padding-left:24px}
ul.adminMenu li a:hover{text-decoration:none; color:#000; background:url(../images/admin_11.png) no-repeat}
ul.adminMenu .On a{color:#fff; background:url(../images/admin_09.png) no-repeat; font-weight:bold}
ul.adminMenu .On a:hover{color:#fff; background:url(../images/admin_09.png) no-repeat}
ul.adminMenu .On ul{display:block}
ul.adminMenu li ul{padding:5px 0px 15px; font-size:1px; line-height:1px}
ul.adminMenu li ul li{font-size:12px; line-height:24px}
ul.adminMenu li ul li a{height:24px; line-height:24px; margin:0; padding-left:24px; border-bottom:1px dotted #ddd; background:none; color:#069; font-weight:normal}
ul.adminMenu li ul li a:hover{background:url(../images/admin_12.png) no-repeat center left}
ul.adminMenu .On ul li a{background:none; color:#069; font-weight:normal}
ul.adminMenu .On ul li a:hover{background:url(../images/admin_12.png) no-repeat center left; color:#069}


