* { margin:0; padding:0 }
img { border:0 }
ul, ol, li, dl, dt, dd { list-style:none }
table { border-collapse:collapse; padding:0; margin:0; border:0; font-size:12px; width:98% }
a { color:#069; text-decoration:none }
a:hover { text-decoration:underline }
div { word-break:break-all; word-wrap:break-word }
body { font-size:12px; line-height:21px; font-family:"瀹嬩綋", "Arial Narrow"; background:#fff }
font { font-size:12px }
.tal { text-align:left }
.tar { text-align:right }
.tac { text-align:center }
.l { float:left }
.r { float:right }
.c { clear:both; width:1px; height:0; line-height:0; font-size:0; overflow:hidden }
.dis { display:block; }
.undis { display:none; }
.fb { font-weight:bold; }
.mBox { width:960px; margin:0 auto }
.tdP15 td { padding:10px 0 7px 15px; border-bottom:1px solid #ccc; color:#069; font-weight:bold }
.pageBoxP { color:#808080; font-size:14px; font-weight:bold; height:40px; margin-top:25px; padding:0 0 0 200px }
.page-cur { background:#efefef; border:#e5e5e5 1px solid; float:left; height:28px; padding:10px 14px 0 }
.page-start { border:#e5e5e5 1px solid; float:left; height:28px; padding:10px 14px 0; width:auto }
.page-break { border-right:#e5e5e5 1px solid; float:left; line-height:40px; padding:0 4px }
.pageBoxP A { border:#e5e5e5 1px solid; border-left:0; float:left; height:28px; padding:10px 14px 0; color:#069 }
.pageBoxP A:hover { background:#efefef }
