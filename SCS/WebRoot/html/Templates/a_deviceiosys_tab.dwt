  <tr>
    <td width="19%">申请人</td>
    <td colspan="2">&nbsp;</td>
    <td width="11%">电话</td>
    <td colspan="2">&nbsp;</td>
    <td width="8%">申请日期</td>
    <td width="16%" colspan="2">&nbsp;</td>
  </tr>
  <tr>
    <td colspan="2">申请人类别</td>
    <td colspan="7">&nbsp;</td>
  </tr>
  <tr>
    <td>类型</td>
    <td colspan="3">&nbsp;</td>
    <td width="11%">申请人密级</td>
    <td colspan="4">&nbsp;</td>
  </tr>
  <tr>
    <td colspan="2">所在部门及处室</td>
    <td colspan="7">&nbsp;</td>
  </tr>
  <tr>
    <td rowspan="3">设备基本情况</td>
    <td width="10%">设备类型</td>
    <td colspan="7">&nbsp;</td>
  </tr>
  <tr>
    <td>设备编号</td>
    <td colspan="3">&nbsp;</td>
    <td width="13%">硬盘序列号</td>
    <td colspan="3">&nbsp;</td>
  </tr>
  <tr>
    <td>设备密级</td>
    <td colspan="3">&nbsp;</td>
    <td>存放地点</td>
    <td colspan="3">&nbsp;</td>
  </tr>
  <tr>
    <td colspan="2">申请人申请开通系统名称</td>
    <td colspan="7">&nbsp;</td>
  </tr>
  <tr>
    <td colspan="2">申请人申请退出系统名称</td>
    <td colspan="7">&nbsp;</td>
  </tr>
  <tr>
    <td colspan="2">申请接入原因</td>
    <td colspan="7">&nbsp;</td>
  </tr>
  <tr>
    <td colspan="2">申请退出原因</td>
    <td colspan="7">&nbsp;</td>
  </tr>
  <tr>
    <td colspan="2" rowspan="2">申请部门意见</td>
    <td colspan="7">&nbsp;</td>
  </tr>
  <tr>
    <td width="12%">签字</td>
    <td colspan="6">&nbsp;</td>
  </tr>
  <tr>
    <td colspan="2" rowspan="2">信息化管理部门意见</td>
    <td colspan="7">&nbsp;</td>
  </tr>
  <tr>
    <td>签字</td>
    <td colspan="6">&nbsp;</td>
  </tr>
