<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.manage.menu.model.Url" table="p_url" >
        <id name="id" type="integer">
            <column name="id"/>
            <generator class="native"/>
        </id>
        <version name="version" type="integer" column="version"/>
        <property name="mlid" type="integer" column = "mlid" />
        <property name="mlurl" type="string">
            <column name="mlURL" length="200"/>
        </property>
    </class>
</hibernate-mapping>