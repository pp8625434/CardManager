<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.manage.table.model.TablesInfo" table="p_tablesinfo" >
     <id name="id" type="java.lang.Integer">
            <column name="ID" />
            <generator class="native" />
        </id>
        <!-- <property name="version" type="java.lang.Integer">
            <column name="VERSION" precision="11" scale="0" />
        </property> -->
        
        <property name="fieldName" type="java.lang.String">
        	<column name="fieldname"/>
        </property>
        
     	<property name="fielDesc" type="java.lang.String">
        	<column name="fieldesc"/>
        </property>
       <property name="tid" type="java.lang.Integer">
        	<column name="tid"/>
        </property>
        
        <property name="fielType" type="java.lang.Integer">
        	<column name="fieltype"/>
        </property>
    </class>
</hibernate-mapping>