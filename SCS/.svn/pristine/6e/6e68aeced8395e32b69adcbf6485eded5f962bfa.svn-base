package com.manage.log.model;

import java.util.Date;

import com.manage.log.model.original.LogAbstract;



public class Log extends LogAbstract{


	
	public Log(){}
	
	public Log(Integer type, Integer function, Integer functionId, 
			String functionName, Integer creator, String creatorName){
		this.type = type;
		this.function = function;
		this.functionId = functionId;
		this.functionName = functionName;
		this.creator = creator;
		this.creatorName = creatorName;
	}
	
	public Log(String id, Integer type, Integer function, Integer functionId, 
			String functionName, Integer creator, String creatorName, Date createDate, String message){
		this.id = id;
		this.type = type;
		this.function = function;
		this.functionId = functionId;
		this.functionName = functionName;
		this.creator = creator;
		this.creatorName = creatorName;
		this.createDate = createDate;
		this.message = message;
	}
}